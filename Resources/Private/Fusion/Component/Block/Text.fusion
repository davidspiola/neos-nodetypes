prototype(Webandco.NodeTypes:Component.Block.Text) < prototype(PackageFactory.AtomicFusion:Content) {

    text = ${q(node).property('text')}
    class = 'text'
    modifier = ''

    @styleguide {
        props {
            text = 'text'
        }
    }

    renderer = Webandco.NodeTypes:Property.Text.Renderer {
        text = ${props.text}
        class = ${props.class}
        modifier = ${props.modifier}
    }
}